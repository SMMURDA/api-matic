{
  "title": "/api/embed",
  "description": "Menemukan sumber video dari halaman web.",
  "method": "GET",
  "path": "/api/embed?url=https://example.com",
  "params": ["url"],
  "response": {
    "items": [
      {
        "tag": "video",
        "src": "https://example.com/video.mp4"
      },
      {
        "tag": "source",
        "src": "https://example.com/video.webm"
      }
    ]
  }
}
